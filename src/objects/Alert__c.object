                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Provider</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Provider Group</fullName>
                <default>false</default>
            </picklistValues>
            <restrictedPicklist>true</restrictedPicklist>
            <sorted>false</sorted>
        </picklist>
        <required>true</required>
        <trackHistory>true</trackHistory>
