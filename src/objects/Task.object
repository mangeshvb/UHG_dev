    <validationRules>
        <fullName>Cell_Number</fullName>
        <active>true</active>
        <errorConditionFormula>NOT( OR( ISBLANK( Cell__c ), REGEX( Cell__c,&quot;(\\D?[0-9]{3}\\D?)[\\s][0-9]{3}-[0-9]{4}&quot;)))</errorConditionFormula>
        <errorDisplayField>Cell__c</errorDisplayField>
        <errorMessage>Enter a valid 10 digit number.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Email</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(OR(ISBLANK(Email__c),(REGEX ( UPPER ( Email__c ) ,&quot;^[A-Z0-9._%+-/!#$%&amp;&apos;*=?^_`{|}~]+@[A-Z0-9.-]+\\.[A-Z]{2,3}$&quot;))))</errorConditionFormula>
        <errorDisplayField>Email__c</errorDisplayField>
        <errorMessage>Enter a valid email format.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Fax</fullName>
        <active>true</active>
