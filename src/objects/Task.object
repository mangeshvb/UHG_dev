    </validationRules>
    <validationRules>
        <fullName>Work_Ext</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(OR(ISBLANK(Work_Ext__c), ISNUMBER(Work_Ext__c)))</errorConditionFormula>
