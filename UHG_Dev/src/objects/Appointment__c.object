    <searchLayouts/>
    <sharingModel>ControlledByParent</sharingModel>
    <validationRules>
        <fullName>cease_prescription_and_fees_for_pending</fullName>
        <active>true</active>
        <errorConditionFormula>(
ISPICKVAL( Appointment_Status__c , &quot;New&quot;)  ||
ISPICKVAL( Appointment_Status__c , &quot;Pending&quot;) ||
ISPICKVAL( Appointment_Status__c , &quot;Rejected&quot;)
)
&amp;&amp;
(
(prescriptions__c != null ) ||
NOT(ISBLANK( Fees__c))
)</errorConditionFormula>
        <errorMessage>You cannot fill prescription or fees till your booking is confirmed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>prescription_and_next_visit_rule</fullName>
        <active>false</active>
        <errorConditionFormula>ISNULL(Fees__c)</errorConditionFormula>
        <errorMessage>the fees cannot be null</errorMessage>
    </validationRules>
